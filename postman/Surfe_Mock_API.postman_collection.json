		{
			"name": "Enrichment",
			"item": [
				{
					"name": "Start Enrichment",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json();",
									"if (jsonData.id) {",
									"    pm.environment.set('enrichmentId', jsonData.id);",
									"    console.log('Saved enrichmentId:', jsonData.id);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"userId\": \"{{contactId}}\"\n}"
						},
						"url": {
							"raw": "{{baseUrl}}/enrichment/start",
							"host": ["{{baseUrl}}"],
							"path": ["enrichment", "start"]
						},
						"description": "Start a new enrichment process. The enrichmentId is automatically saved to the environment for use in subsequent requests."
					},
					"response": []
				},
				{
					"name": "Get Enrichment Status (Dynamic)",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/enrichment/{{enrichmentId}}",
							"host": ["{{baseUrl}}"],
							"path": ["enrichment", "{{enrichmentId}}"]
						},
						"description": "Get the status of the enrichment that was started. Poll this endpoint to watch the status change from pending -> in_progress -> completed."
					},
					"response": []
				},
				{
					"name": "Get Enrichment - Static Pending",
